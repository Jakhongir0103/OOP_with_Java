package supermarket;

import java.util.ArrayList;
import java.util.List;

public class Product {
	private String categoryName;
	private String productNames;
	private double productPrices;
	private List<Integer> discount = new ArrayList<>();
	
	public Product(String categoryName, String productNames, double d) {
		super();
		this.categoryName = categoryName;
		this.productNames = productNames;
		this.productPrices = d;
		this.discount.add(0);
	}
	public String getCategoryName() {
		return categoryName;
	}
	public String getProductNames() {
		return productNames;
	}
	public double getProductPrices() {
		return productPrices;
	}
	public void setDiscount(int dis){
		this.discount.add(dis);
	}
	public List<Integer> getDiscount() {
		return discount;
	}
}
