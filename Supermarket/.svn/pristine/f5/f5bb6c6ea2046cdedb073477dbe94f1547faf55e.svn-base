package supermarket;

public class Card {
	private int code;
	private String name;
	private String dateOfBirth;
	private int avblPoints = 0;
	private int totalPoints = 0;
	public Card(int code, String name, String dateOfBirth) {
		this.code = code;
		this.name = name;
		this.dateOfBirth = dateOfBirth;
	}
	public int getAvlblPoints() {
		return avblPoints;
	}
	public void setAvlblPoints(int avPoints) {
		this.avblPoints = avPoints;
	}
	public int getTotalPoints() {
		return totalPoints;
	}
	public void addTotalPoints(int totalPoints) {
		this.totalPoints += totalPoints;
	}
	public String getName() {
		return name;
	}
	public String getDateOfBirth() {
		return dateOfBirth;
	}
	public int getCode() {
		return this.code;
	}
}
